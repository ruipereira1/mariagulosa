  "description": "Site de bolos artesanais Maria Gulosa",
  "main": "index.js",
  "scripts": {
    "dev": "vercel dev --yes",
    "build": "cd frontend && npm run build",
    "start": "vercel dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "cd frontend && npm install",
    "frontend": "cd frontend && npm run dev",
